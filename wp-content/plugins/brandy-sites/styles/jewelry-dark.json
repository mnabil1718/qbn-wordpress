{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "title": "Jewelry Dark styles",
  "description": "Brandy Jewelry Dark site",
  "settings": {
    "color": {
      "palette": [
        {
          "color": "#377E62",
          "slug": "brandy-accent",
          "name": "Brandy accent"
        },
        {
          "color": "#D6E0E9",
          "slug": "brandy-primary-text",
          "name": "Brandy primary text"
        },
        {
          "color": "#5A6D80",
          "slug": "brandy-secondary-text",
          "name": "Brandy secondary text"
        },
        {
          "color": "#182937",
          "slug": "brandy-border",
          "name": "Brandy border"
        },
        {
          "color": "#060E16",
          "slug": "brandy-theme-background",
          "name": "Brandy theme background"
        },
        {
          "color": "#122941",
          "slug": "brandy-primary-black",
          "name": "Brandy primary black"
        },
        {
          "color": "#0A1B2B",
          "slug": "brandy-theme-secondary-background",
          "name": "Brandy theme secondary background"
        },
        {
          "color": "#000000",
          "slug": "brandy-theme-third-background",
          "name": "Brandy theme third background"
        },
        {
          "color": "#0A1B2B",
          "slug": "brandy-theme-fourth-background",
          "name": "Brandy theme fourth background"
        },
        {
          "color": "#143351",
          "slug": "brandy-theme-fifth-background",
          "name": "Brandy theme fifth background"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Prata",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": [
                "file:./[brandy_sites_path]assets/fonts/prata/prata-regular.ttf"
              ]
            }
          ],
          "fontFamily": "\"Prata\", sans-serif",
          "name": "Prata",
          "slug": "prata"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Outfit",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": ["file:./assets/fonts/outfit/outfit-regular.ttf"]
            }
          ],
          "fontFamily": "\"Outfit\", sans-serif",
          "name": "Outfit",
          "slug": "outfit"
        },
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
          "slug": "system-fonts",
          "name": "System fonts"
        }
      ]
    },
    "layout": {
      "contentSize": "960px",
      "wideSize": "1440px"
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var:preset|font-family|outfit"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|prata"
        }
      },
      "button": {
        "border": {
          "radius": "5px"
        },
        "spacing": {
          "padding": {
            "left": "30px",
            "right": "30px",
            "top": "17px",
            "bottom": "17px"
          }
        },
        "typography": {
          "fontSize": "16px",
          "fontWeight": "500",
          "lineHeight": "1"
        }
      }
    },
    "blocks": {
      "core/tag-cloud": {
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|brandy-secondary-text",
              "background": "#000000"
            },
            "border": {
              "radius": "7px"
            },
            "spacing": {
              "margin": {
                "top": "0px",
                "left": "0px",
                "right": "0px",
                "bottom": "0px"
              },
              "padding": {
                "top": "0.5rem",
                "bottom": "0.5rem",
                "left": "0.75rem",
                "right": "0.75rem"
              }
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|brandy-primary-text"
              }
            }
          }
        },
        "variations": {
          "outline": {
            "typography": {
              "fontSize": "14px"
            }
          }
        }
      }
    }
  }
}
